# PostgreSQL connection settings
DATABASE_URL=postgresql://user:password@db/eve_market
POSTGRES_USER=user
POSTGRES_PASSWORD=password
POSTGRES_DB=eve_market

# The log level for the application. Can be DEBUG, INFO, WARNING, ERROR, CRITICAL.
LOG_LEVEL=INFO

# The API_KEY is optional. If you leave it blank, the /api/refresh endpoint will not be secured.
# To secure it, uncomment the line below and set a secret key.
# API_KEY=your-secret-api-key

# Redis connection URL for caching
REDIS_URL=redis://redis:6379

# Cloudflare Tunnel Token
# Create a tunnel in the Cloudflare Zero Trust dashboard and paste the token here.
TUNNEL_TOKEN=

# The public URL for the backend API, exposed via the Cloudflare Tunnel.
# This must be configured in your Cloudflare dashboard for your tunnel.
# Example: https://my-api-tunnel.mydomain.com/api
NEXT_PUBLIC_API_BASE_URL=